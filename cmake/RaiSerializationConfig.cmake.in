@PACKAGE_INIT@

# If the package was installed with the test helper, consumers may link
# to `RaiSerialization::RaiSerializationTest` to get access to test utilities
# (implemented in `JsonTestHelper.cppm`). When installed, the config will
# include the GTest dependency automatically (via find_dependency(GTest)).
if(@RAISERIALIZATION_INSTALL_TEST_HELPER@)
	include(CMakeFindDependencyMacro)
	find_dependency(GTest)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/RaiSerializationTargets.cmake")

set(RAISERIALIZATION_CXX_STANDARD 23)

check_required_components(RaiSerialization)
